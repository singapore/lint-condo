    "stylelint": "8.2.0",
    "tern-lint": "0.6.0",
    "tslint": "5.8.0",
    "typescript": "2.5.3",
    "write-good": "0.11.3",
    "xo": "0.18.2"
  },
  "devDependencies": {
    "glob": "7.1.2",
    "js-yaml": "3.10.0",
    "log-symbols": "2.1.0"
